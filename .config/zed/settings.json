{
  "icon_theme": {
    "mode": "system",
    "light": "Zed (Default)",
    "dark": "Zed (Default)",
  },
  "colorize_brackets": true,
  "buffer_line_height": {
    "custom": 2.0,
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false,
  },
  "show_edit_predictions": true,
  "edit_predictions": {
    "mode": "eager",
  },
  "features": {
    "edit_prediction_provider": "copilot",
  },
  "agent": {
    "default_model": {
      "provider": "copilot_chat",
      "model": "claude-haiku-4.5",
    },
    "model_parameters": [],
  },
  "buffer_font_family": "VictorMono Nerd Font",
  "buffer_font_size": 15.0,
  "buffer_font_weight": 600.0,
  "cursor_blink": false,
  "relative_line_numbers": "enabled",
  "ui_font_size": 16,
  "vim_mode": true,
  "theme": {
    "mode": "light",
    "light": "VSCode Light+",
    "dark": "Gruvbox Dark",
  },
  "title_bar": {
    "show_branch_icon": true,
    "show_sign_in": false,
  },
  "toolbar": {
    "breadcrumbs": false,
    "quick_actions": false,
  },
  "status_bar": {
    "active_language_button": false,
  },
  "terminal": {
    "copy_on_select": true,
    "font_size": 13,
    "font_weight": 500,
    "toolbar": {
      "breadcrumbs": false,
    },
  },
  "git": {
    "inline_blame": {
      "enabled": false,
    },
  },
  "languages": {
    "TypeScript": {
      "language_servers": ["tsgo", "vtsls"],
    },
    "Python": {
      "language_servers": ["ty", "basedpyright"],
    },
  },
}
