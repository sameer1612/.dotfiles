{
  "buffer_font_family": "VictorMono Nerd Font",
  "buffer_font_size": 15,
  "buffer_font_weight": 600,
  "cursor_blink": false,
  "relative_line_numbers": true,
  "ui_font_size": 16,
  "vim_mode": true,
  "features": {
    "edit_prediction_provider": "copilot"
  },
  "theme": {
    "mode": "system",
    "light": "One Light",
    "dark": "One Dark"
  },
  "toolbar": {
    "breadcrumbs": false,
    "quick_actions": false
  }
}
