{
  "show_edit_predictions": true,
  "edit_predictions": {
    "mode": "eager"
  },
  "agent": {
    "default_model": {
      "provider": "copilot_chat",
      "model": "gpt-5-mini"
    },
    "model_parameters": []
  },
  "buffer_font_family": "VictorMono Nerd Font",
  "buffer_font_size": 16,
  "buffer_font_weight": 600,
  "cursor_blink": false,
  "relative_line_numbers": true,
  "ui_font_size": 16,
  "tab_size": 2,
  "vim_mode": true,
  "theme": {
    "mode": "system",
    "light": "VSCode Light+",
    "dark": "One Dark"
  },
  "title_bar": {
    "show_branch_icon": true,
    "show_sign_in": false
  },
  "toolbar": {
    "breadcrumbs": false,
    "quick_actions": false
  },
  "status_bar": {
    "active_language_button": false
  },
  "terminal": {
    "copy_on_select": true,
    "font_size": 13,
    "font_weight": 500,
    "toolbar": {
      "breadcrumbs": false
    }
  },
  "git": {
    "inline_blame": {
      "enabled": false
    }
  }
}
